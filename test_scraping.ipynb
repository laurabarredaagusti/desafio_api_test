    "from bs4 import BeautifulSoup as bs\n",
    "import requests\n",
    "\n",
    "url = 'https://tarifaluzhora.es/'\n",
    "\n",
    "response = requests.get(url)\n",
    "\n",
    "html = response.content\n",
    "\n",
    "soup = bs(html, \"lxml\")\n",
    "\n",
    "precio = float(soup.find('span', class_='main_text').text[:-2])"
   ]
  }
